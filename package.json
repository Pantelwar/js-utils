    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.4",
    "@types/jest": "^29.5.5",
    "@types/mime-types": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
